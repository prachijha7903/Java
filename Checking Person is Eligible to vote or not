import java.util.*;

public class ifElseLoop {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter your Age : ");
        int age = sc.nextInt();

      if(age>=18){
            System.out.println("Can Vote");
        }else{
            System.out.println("Cannot Vote");
        }
    }
}
